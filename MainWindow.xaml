<Window x:Class="Archiver.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Archiver"
        mc:Ignorable="d"
        Title="Archiver" Height="450" Width="800">
    <Grid>
        <TreeView x:Name="treeView" Width="400" HorizontalAlignment="Left"/>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Right" Width="400">
            <Button x:Name="exportBtn" Content="Export" Margin="10" Click="exportBtn_Click" />
            <Button x:Name="importBtn" Content="Import" Margin="10" Click="importBtn_Click" />
        </StackPanel>
    </Grid>
</Window>
